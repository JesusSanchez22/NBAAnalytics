<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="445.0" prefWidth="280.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.nbaanalytics.adminIndexController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="469.0" prefWidth="250.0">
        <children>
            <AnchorPane layoutY="-1.0">
                <children>
                    <ImageView fitHeight="489.0" fitWidth="299.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../../images/fondo.jpg" />
                        </image>
                    </ImageView>
                    <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="60.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../../images/logoNBA.png" />
                        </image>
                    </ImageView>
                    <Button fx:id="btnEquipos" layoutX="43.0" layoutY="303.0" mnemonicParsing="false" onAction="#goEquipos" prefHeight="97.0" prefWidth="192.0" text="EQUIPOS" textFill="#0059b3">
                        <font>
                            <Font name="Arial Black" size="20.0" />
                        </font>
                    </Button>
                    <Button fx:id="btnJugadas" layoutX="42.0" layoutY="167.0" mnemonicParsing="false" onAction="#goJugadores" prefHeight="97.0" prefWidth="192.0" text="JUGADORES" textFill="#0059b3">
                        <font>
                            <Font name="Arial Black" size="20.0" />
                        </font>
                    </Button>
                </children>
            </AnchorPane>
        </children>
    </AnchorPane>
</VBox>
